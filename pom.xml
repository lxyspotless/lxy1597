  <build>
    <finalName>NEO-LOAN-PMS</finalName>
	<resources>
       <resource>
           <directory>src/main/java</directory>
           <includes>
               <include>**/*.xml</include>
           </includes>
           <!-- 打包时是否过滤该类型资源 -->
           <filtering>false</filtering>
       </resource>
       <resource>
           <directory>src/main/resources</directory>
           <includes>
               <include>**/*.properties</include>
               <include>**/*.xml</include>
           </includes>
           <!-- 打包时是否过滤该类型资源 -->
           <filtering>false</filtering>
       </resource>
	</resources>
    <plugins>
      <plugin>
        <artifactId>maven-compiler-plugin</artifactId>
        <configuration>
          <source>1.8</source>
          <target>1.8</target>
          <encoding>UTF-8</encoding>
        </configuration>
      </plugin>
      <plugin>
	      <groupId>org.apache.maven.plugins</groupId>
	      <artifactId>maven-jar-plugin</artifactId>
	      <version>2.3.1</version>
	      <executions>
	      	  <!-- 同时打成jar包 -->
	          <execution>
	              <id>NEO-LOAN-PMS</id>
	              <phase>package</phase>
	              <goals>
	                  <goal>jar</goal>
	              </goals>
	          </execution>
	      </executions>
	  </plugin>
    </plugins>
  </build>
</project>
